{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "pocket",
  "version": "0.1.0",
  "identifier": "rewant.pocket",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Pocket",
        "width": 1000,
        "height": 700,
        "resizable": true
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": ["msi", "nsis"],
    "icon": [
      "icons/pocket.ico",
      "icons/icon.ico"
    ],
    "windows": {
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    }
  },
  "plugins": {
    "updater": {
      "active": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDQ5MEM0MjI4RjQ3NjM3RjYKUldSWFk5V2pJQk1QSWN6N2VnWVpQSlRZM2FJZ0lJc0FZd0FvRUFnQlVBMHZDSEJ3T2k4N2VnWVpQSlRZM2FJZ0lJc0FZd0FvRUFnQlVB"
    }
  }
}